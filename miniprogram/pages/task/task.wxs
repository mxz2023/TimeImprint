
var getEventTitle = function(event) {
  return event.eventTitle
}

var getEventTime = function(event) {
  return event.eventTime
}

var getEventItemContent = function(event, index) {
  contents = event.eventContent
  item = contents[index]
  if (item) {
    value = item.content
    return value
  }
  return ""
}


var getTaskTitle = function(task) {
  return task.taskTitle
}

module.exports = {
  eventTitle: getEventTitle,
  eventTime: getEventTime,
  eventItemContent: getEventItemContent,
  taskTitle: getTaskTitle,
}