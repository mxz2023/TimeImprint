<!--pages/main/main.wxml-->

<m-home wx:if="{{selectIndex == 0}}" class="mBox" style="padding-top: {{stateBarHeight}}px;"></m-home>

<view wx:elif="{{selectIndex == 1}}" class="mBox" ></view>

<view wx:elif="{{selectIndex == 2}}" class="mBox" ></view>

<view wx:else class="mBox"></view>

<view class="wrapper">
  <m-tab-bar bind:updateSelectIndex="onUpdateSelectIndex" list="{{list}}"></m-tab-bar>
</view>
